{
    "extends": "tslint:recommended",
    "rules": {
        "indent": [
            true,
            "spaces"
        ],
        "no-console": false,
        "ordered-imports": [
            true,
            {
                "grouped-imports": true
            }
        ],
        "object-literal-sort-keys": false,
        "interface-name": false,
        "max-line-length": false,
        "no-string-literal": false,
        "no-empty": [true, "allow-empty-functions"]
    }
}